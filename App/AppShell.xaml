<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TMS_APP.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TMS_APP.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="TMS_APP">

    <ShellContent
        ContentTemplate="{DataTemplate local:AccessPortal}"
        Route="AccessPortal" />

    <ShellContent
        ContentTemplate="{DataTemplate local:Hub}"
        Route="Hub" />

</Shell>
